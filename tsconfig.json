{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/profile.js",
    "app/(tabs)/personal-details.js",
    "app/daily-tracking.js",
    "app/(tabs)/calendar.tsx"
  ]
}
